    <item>
      <title>HUST硬件综合训练</title>
      <link>https://zundadd.github.io/HasekawaMikan.github.io/posts/%E6%9D%82%E8%B0%88/hardcoretrain/</link>
      <pubDate>Sun, 31 Aug 2025 00:00:00 +0000</pubDate>
